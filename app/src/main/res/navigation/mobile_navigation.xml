<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.andao_apk.Categorie.CategorieFragment"
        android:label="Andao"
        tools:layout="@layout/fragment_categorie" />

    <fragment
        android:id="@+id/navigation_multimedia"
        android:name="com.example.andao_apk.Multimedia.MultimediaFragment"
        android:label="multimedia"
        tools:layout="@layout/fragment_multimedia" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.andao_apk.ui.dashboard.DashboardFragment"
        android:label="reglages"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.example.andao_apk.Notification_android.NotificationFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/navigation_partage"
        android:name="com.example.andao_apk.ui.notifications.NotificationsFragment"
        android:label="partage"
        tools:layout="@layout/fragment_notifications" />
</navigation>